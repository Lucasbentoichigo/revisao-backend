generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

model Foods {
    id          Int      @id @default(autoincrement())
    name        String
    description String
    price       Decimal
    category    String
    available   Boolean
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt

    @@map("Foods")
}
